                <!-- Header -->
                <div class="text-center mb-20">
                    <div class="inline-block">
                        <div class="flex items-center gap-3 mb-4">
                            <div
                                class="h-1 w-12 bg-gradient-to-r from-transparent to-cmap-naranja-600"
